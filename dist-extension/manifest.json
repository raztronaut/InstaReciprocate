{
  "manifest_version": 3,
  "name": "InstaReciprocate",
  "version": "1.0.0",
  "description": "Analyze your Instagram following and followers with advanced insights. Safe, private, and feature-rich Instagram relationship analyzer.",
  "author": "InstaReciprocate",
  "homepage_url": "https://github.com/yourusername/instagram-analytics",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.instagram.com/*"
  ],
  "action": {
    "default_title": "InstaReciprocate"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["icons/*"],
    "matches": ["https://www.instagram.com/*"]
  }],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "minimum_chrome_version": "88",
  "offline_enabled": true,
  "short_name": "InstaReciprocate"
} 